  <form class="container" method="POST" action="https://formspree.io/cjdomaleski@gmail.com">
    <div class="row">
          <!-- input field for first name-->
      <div class="input-field col s6">
        <i class="material-icons prefix">account_circle</i>
        <input id="firstName" type="text" class="validate" required="" aria-required="true" name="FirstName">
        <label for="firstName">First Name</label>
      </div>
      <!-- input field for last name-->
      <div class="input-field col s6">
        <i class="material-icons prefix">account_circle</i>
        <input id="lastName" type="text" class="validate" required="" aria-required="true" name="LastName">
        <label for="lastName">Last Name</label>
      </div>
    </div>

      <div class="row">
        <!-- input field for Phone number-->
        <div class="input-field col s6">
          <i class="material-icons prefix">phone</i>
          <input id="phoneNumber" type="tel" class="validate" required="" aria-required="true" pattern="^\d{3}-\d{3}-\d{4}$" name="Number">
          <label for="phoneNumber">Telephone(format: xxx-xxx-xxxx)</label>
        </div>
          <!-- input field for email-->
          <div class="input-field col s6">
            <i class="material-icons prefix">markunread_mailbox</i>
            <input type="email" id="email" type="tel" class="validate" required="" aria-required="true" name="Email">
            <label for="email" data-error="Please enter a vailed email address" data-success="">Email</label>
          </div>
        </div>

<!--Check what locations you're interested in-->
        <div class="row">
          <div class="input-field col s6 m6">
            <i class="material-icons prefix">add_location</i>
            <select id="location" name="location" required="required" class="validate" >
              <option value="" disabled selected>Choose your Location</option>
              <option value="Lake Oconee">Lake Oconee</option>
              <option value="Lake Sinclair">Lake Sinclair</option>
            </select>
            <label data-error="Select an option" data-success=""></label>
          </div>
          <br>
          <br>
          <br>

<!--Check what vechiles you're interested in-->

          <p>Check what vechiles you are intersted in</p>
          <input type="checkbox" class="filled-in" id="PontoonCheckBox" name="PontoonCheck" />

          <label for="PontoonCheckBox">Pontoon</label>

          <br>

          <input type="checkbox" class="filled-in" id="SkiBoatCheckBox" name="SkiBoatCheck"/>
          <label for="SkiBoatCheckBox">Ski boat</label>

          <br>

          <input type="checkbox" class="filled-in" id="JetSkiCheckBox" name="JetSkiCheck"/>
          <label for="JetSkiCheckBox">Jet Ski</label>
</div>




<br>


        <div class="row">
