<script>
$(document).ready(function() {

   $(".button-collapse").sideNav();

    $('select').material_select();

    $('.collapsible').collapsible();

});
</script>

<script>
$("select[required]").css({display: "block", height: 0, padding: 0, width: 0, position: 'absolute'});

//This function controlls the google maps their api is here https://developers.google.com/maps/documentation/javascript/ 
$(".collapsible-header").one("click", function(){

                            var myCenter1 = new google.maps.LatLng(33.412588, -83.280146);
                            var mapCanvas1 = document.getElementById("map1");
                            var mapOptions1 = {      //mapOptions defines the properties for the map
                                    center: new google.maps.LatLng(33.412588, -83.280146),  //center tells the map where to place the marker according to the lat. and long.
                                    zoom: 16,       //specifies the zoom level for the map
                                    mapTypeId: google.maps.MapTypeId.ROADMAP //speciifies the type of map to display (ROADMAP, SATELLITE, HYBRID, and TERRAIN) are the usable map types
                            }
                            var map1 = new google.maps.Map(mapCanvas1, mapOptions1);
                            var marker1 = new google.maps.Marker({
                              position:myCenter1,
                              //animation:google.maps.Animation.BOUNCE    //makes the pin bounce up and down on the map
                            });
                            marker1.setMap(map1);

                            var infoWindow1 = new google.maps.InfoWindow({    //creates an information window on the map
                              content: "Fish Tale Marina, 891 Greensboro Rd, Eatonton Ga. 31024"
                            });
                            infoWindow1.open(map1, marker1);

                            //--------------------------------------------------------------------------------------------------

                            var myCenter2 = new google.maps.LatLng(33.184465, -83.290489);
                            var mapCanvas2 = document.getElementById("map2");
                            var mapOptions2 = {      //mapOptions defines the properties for the map
                                    center: new google.maps.LatLng(33.184465, -83.290489),  //center tells the map where to place the marker according to the lat. and long.
                                    zoom: 16,       //specifies the zoom level for the map
                                    mapTypeId: google.maps.MapTypeId.ROADMAP //speciifies the type of map to display (ROADMAP, SATELLITE, HYBRID, and TERRAIN) are the usable map types
                            }
                            var map2 = new google.maps.Map(mapCanvas2, mapOptions2);
                            var marker2 = new google.maps.Marker({
                              position:myCenter2,
                              //animation:google.maps.Animation.BOUNCE  //makes the pin bounce up and down on the map
                            });
                            marker2.setMap(map2);

                            var infoWindow2 = new google.maps.InfoWindow({    //creates an information window on the map
                              content: "Little River Marina, 3066 N Columbia St, Milledgeville, Ga. 31061"
                            });
                            infoWindow2.open(map2, marker2);

});
</script>
